<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
 
  
  <settings>
  	<setting name="jdbcTypeForNull" value="NULL"/>
  	<!-- 파라미터전달된 데이터가 null인 경우, db에서도 null로 인식할 것 -->
  	<setting name="mapUnderscoreToCamelCase" value="true"/>
  	<!-- 컬럼명에 _를 사용한 경우, java코드에서는 camelCasing으로 치환할 것 -->

  </settings>
  
  <typeAliases>
  	<typeAlias type="com.kh.myhouse.agent.model.vo.Agent" alias="agent"/>
	<typeAlias type="com.kh.myhouse.estate.model.vo.Estate" alias="estate"/>
	<typeAlias type="com.kh.myhouse.estate.model.vo.EstateAttach" alias="estateAttach"></typeAlias>
	<typeAlias type="com.kh.myhouse.estate.model.vo.Option" alias="option"/>
	<typeAlias type="com.kh.myhouse.common.typehandler.StrArrTypeHandler" 
  			   alias="strArrTypeHandler"/>
	<typeAlias type="com.kh.myhouse.member.model.vo.Member" alias="member"/>
	<typeAlias type="com.kh.myhouse.interest.model.vo.Interest" alias="interest"/>
	<typeAlias type="com.kh.myhouse.cart.model.vo.Cart" alias="cart"/>
	<typeAlias type="com.kh.myhouse.chat.model.vo.Msg" alias="msg"/>
  </typeAliases>
  
<!--   <mappers>
  	<mapper resource="/mappers/chat/chat-mapper.xml"/>
  </mappers> -->

</configuration>